        <obo:IAO_0000412 xml:lang="en">OMIT</obo:IAO_0000412>
        <obo:IAO_0000412 xml:lang="en">re3data</obo:IAO_0000412>
        <rdfs:comment xml:lang="en">Theology is the study or science which is concerned with God, his nature and attributes, and his relations with man and the universe. [Adapted from OED, AL 6.6.2019]</rdfs:comment>
        <rdfs:label xml:lang="en">Theology</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMIT_0026522 -->

