        <rdfs:subClassOf rdf:resource="http://edamontology.org/topic_3318"/>
        <rdfs:comment>Meteorology is the branch of science that deals with atmospheric phenomena and processes, especially with a view to forecasting the weather. [AL, modified from OED, accessed 26.11.18]</rdfs:comment>
        <rdfs:label xml:lang="en">Meteorology</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMIT_0026615 -->

